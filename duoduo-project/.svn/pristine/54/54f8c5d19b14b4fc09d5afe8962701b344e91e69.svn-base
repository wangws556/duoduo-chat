                <ResourceDictionary Source="Resources\DataTemplate\AgentPortal\BuyMembershipDT.xaml"/>
                <ResourceDictionary Source="Resources\DataTemplate\AgentPortal\DepositDT.xaml"/>
                <ResourceDictionary Source="Resources\DataTemplate\AgentPortal\DianCardSaleDT.xaml"/>
                <ResourceDictionary Source="Resources\DataTemplate\AgentPortal\DianCardStocksDT.xaml"/>
                <ResourceDictionary Source="Resources\DataTemplate\AgentPortal\HosterDT.xaml"/>
                <ResourceDictionary Source="Resources\DataTemplate\AgentPortal\LogoffDT.xaml"/>
                <ResourceDictionary Source="Resources\DataTemplate\AgentPortal\PasswordDT.xaml"/>
                <ResourceDictionary Source="Resources\DataTemplate\AgentPortal\UserCashDT.xaml"/>
                <ResourceDictionary Source="Resources\Selector\AgentPortalSelector.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
