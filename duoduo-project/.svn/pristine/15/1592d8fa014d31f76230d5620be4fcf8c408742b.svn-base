html, body
{
    background-color: #deebff;
    overflow: hidden;
    width: 100%;
    height: 100%;
    padding: 0;
    margin: 0;
}

li
{
    width: 200px;
}

#roomGroupTab div
{
    background-color: #deebff;
}


#roomGroupTabHeader li
{
    display: inline-table;
    height: 30px;
    width: 120px;
    margin: 1px 2px;
    padding: 0 5px;
    text-align: center;
    border: 2px solid blue;
}
    

.roomGroupTree 
{
    background-color: #deebff;
}

    .roomGroupTree div
    {
        font-size: 14px;
        background-color: #deebff;
    }

.rankTab li
{
    display: inline-table;
    height: 30px;
    width: 75px;
    margin: 1px 2px;
    padding: 0 5px;
    font-size: 12px;
    text-align: center;
}

.searchBox
{
    border: 2px solid #6ea3f2;
    width: 170px;
}

.rb1
{
    margin-left: 5px;
    border-right: #6a6a6a 1px solid;
    border-top: #fff 1px solid;
    font-size: 12px;
    background: #d32c47;
    border-left: #fff 1px solid;
    color: #fff;
    border-bottom: #6a6a6a 1px solid;
    height: 24px;
    width: 35px;
}

.rb2
{
    margin-left: 2px;
    border-right: #6a6a6a 1px solid;
    border-top: #fff 1px solid;
    font-size: 12px;
    background: #C71E3A;
    border-left: #fff 1px solid;
    border-bottom: #6a6a6a 1px solid;
    height: 24px;
    width: 35px;
}

div.roomBlock
{
    width: 130px;
    height: 180px;
    z-index: 101;
    list-style: none;
    text-align: center;
   
    display: inline;
    border:#909090 1px solid;
    background-color:white;
    filter:progid:DXImageTransform.Microsoft.Shadow(color=#909090,direction=120,strength=4);
    -moz-box-shadow: 2px 2px 10px #909090;
    -webkit-box-shadow: 2px 2px 10px #909090;
    box-shadow:2px 2px 10px #909090;
    position:absolute
}

.roomImage
{
    position: absolute;
    top: 0;
    left: 0;
    width: 130px;
    height: 120px;
}

.roomText
{
    position: absolute;
    left: 0px;
    top: 120px;
    width: 130px;
    height: 50px;
    background-color: white;
}

.roomText span
{
    border-radius: 3px 3px 3px 3px;
    display: block;
    font-size: 12px;
    margin-top: 5px;
    padding: 0;
    color: black;
    width: 130px;
    overflow: visible;
    text-align: left;
    z-index: 1;
}

.outerRoomDiv
{
    width: 130px;
    height: 180px;
    display:inline;
    position:relative;
    margin: 10px;
    background-color:white;
}

