div.roomText span:focus
{
    background-color: lightgreen;
}

.location
{
    font-size: 14px;
    color: blue;
    font-weight: bold;
    vertical-align: middle;
}

.toolbarWindow
{
    border: 0px;
    padding: 0px;
    display: none;
    overflow-y: hidden;
    overflow-x: hidden;
    border:solid;
    background:#9CAEF5;
}


.toolbarWindowContent
{
    overflow: hidden;
    width: 100%;
    height: 100%;
    background-color: #9CAEF5;
    margin: 0;
    padding-top: 0;
}

.toolbarWindowHeader
{
    visibility: hidden;
    height: 1px;
    background-color: transparent;
    margin: 0;
    padding: 0;
}

