div.roomBlock
{
    width: 150px;
    cursor: pointer;
    height: 210px;
    z-index: 101;
    list-style: none;
    text-align: center;
    position: relative;
    
    display: inline;
    border:#909090 1px solid;
    background-color:white;
    filter:progid:DXImageTransform.Microsoft.Shadow(color=#909090,direction=120,strength=4);
    -moz-box-shadow: 2px 2px 10px #909090;
    -webkit-box-shadow: 2px 2px 10px #909090;
    box-shadow:2px 2px 10px #909090;
    position:absolute
}

.roomImage
{
    position: absolute;
    top: 0;
    left: 0;
    width: 150px;
    height: 160px;
}

.roomText
{
    position: absolute;
    left: 0px;
    top: 150;
    width: 150px;
    height: 50px;
    z-index: 1;
    background-color: transparent;
}

    .roomText span
    {
        border-radius: 3px 3px 3px 3px;
        display: block;
        font-size: 12px;
        margin-top: 5px;
        padding: 0;
        color: black;
        width: 160px;
        overflow: visible;
        text-align: left;
        z-index: 1;
    }

.outerRoomDiv
{
    width: 150px;
    height: 210px;
    display:inline;
    position:relative;
    margin: 10px;
    padding: 2px;
}

.roomBox
{
    display:none;
    height:160px;
    width:150px;
    z-index:102;
    background-color:black;
    filter:alpha(Opacity=80);
    -moz-opacity:0.5;
    opacity: 0.5;
    position:absolute
}

.Circle
{
    position:absolute;
    display:none; 
    border:1px solid #f60; width:100px; height:100px; margin:0 auto; 
    -webkit-border-radius:50px; 
    -moz-border-radius:50px; 
    -o-border-radius:50px; 
    border-radius:50px; 
    z-index:103;
} 

.Rectangle
{  
    position:absolute;
    display:none;
    width:0px; 
    height:0px; 
    margin-top:25px;
    margin-left:25px; 
    border:15px solid transparent; 
    border-left-color:#f00;  
    border-right-color:transparent; 
    border-top-color:transparent; 
    border-bottom-color:transparent; 
    z-index:104;
}

div.roomText span:focus
{
    background-color: lightgreen;
}

.location
{
    font-size: 14px;
    color: blue;
    font-weight: bold;
    vertical-align: middle;
}

.toolbarWindow
{
    border: 0px;
    padding: 0px;
    display: none;
    overflow-y: hidden;
    overflow-x: hidden;
    border:solid;
    background:#9CAEF5;
}


.toolbarWindowContent
{
    overflow: hidden;
    width: 100%;
    height: 100%;
    background-color: #9CAEF5;
    margin: 0;
    padding-top: 0;
}

.toolbarWindowHeader
{
    visibility: hidden;
    height: 1px;
    background-color: transparent;
    margin: 0;
    padding: 0;
}

