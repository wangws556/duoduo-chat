div.roomBlock
{
    width: 130px;
    height: 180px;
    z-index: 101;
    list-style: none;
    text-align: center;
   
    display: inline;
    border:#909090 1px solid;
    background-color:white;
    filter:progid:DXImageTransform.Microsoft.Shadow(color=#909090,direction=120,strength=4);
    -moz-box-shadow: 2px 2px 10px #909090;
    -webkit-box-shadow: 2px 2px 10px #909090;
    box-shadow:2px 2px 10px #909090;
    position:absolute
}

.roomImage
{
    position: absolute;
    top: 0;
    left: 0;
    width: 130px;
    height: 130px;
}

.roomText
{
    position: absolute;
    left: 0px;
    top: 130px;
    width: 130px;
    height: 50px;
    background-color: white;
}

.roomText span
{
    border-radius: 3px 3px 3px 3px;
    display: block;
    font-size: 12px;
    margin-top: 5px;
    padding: 0;
    color: black;
    width: 130px;
    overflow: visible;
    text-align: left;
    z-index: 1;
}

.outerRoomDiv
{
    width: 130px;
    height: 180px;
    display:inline;
    position:relative;
    margin: 10px;
    background-color:white;
}

.playBtn
{ 
    display:none; 
    position: relative; 
    border: 65px solid black; 
    color: black; 
    height: 180px; 
    width:0px;
    filter:alpha(Opacity=70);
     -moz-opacity:0.7;
     opacity: 0.7;
    -webkit-border-radius: 100%; 
    -moz-border-radius: 100%; 
    -o-border-radius: 100%; 
    border-radius: 100%;
    z-index:103;
}  

.playBtn a { border-style: solid; text-indent: -9999px; position: absolute; top: 0px; left: -5px;  
             border-color: transparent transparent transparent orange; border-width: 18px,0px,18px,45px;  width: 0px; height: 0px;line-height:0px; }
    .playBtn span
    {
        border-radius: 3px 3px 3px 3px;
        display: block;
        font-size: 12px;
        padding: 0;
        color: red;
        margin-top:45px;
        margin-left:-45px;
        width: 100px;
        overflow: visible;
        text-align: left;
        z-index: 1;
        background-color:black;
        position:absolute;
    }

div.roomText span:focus
{
    background-color: lightgreen;
}

.location
{
    font-size: 14px;
    color: blue;
    font-weight: bold;
    vertical-align: middle;
}

.toolbarWindow
{
    border: 0px;
    padding: 0px;
    display: none;
    overflow-y: hidden;
    overflow-x: hidden;
    border:solid;
    background:#9CAEF5;
}


.toolbarWindowContent
{
    overflow: hidden;
    width: 100%;
    height: 100%;
    background-color: #9CAEF5;
    margin: 0;
    padding-top: 0;
}

.toolbarWindowHeader
{
    visibility: hidden;
    height: 1px;
    background-color: transparent;
    margin: 0;
    padding: 0;
}

